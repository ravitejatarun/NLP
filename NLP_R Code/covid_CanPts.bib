  volume    =  44,
  number    =  102200,
  pages     = "102200",
  month     =  nov,
  year      =  2021,
  keywords  = "ApoA; ApoB; Mortality; Prognosis; Sars-Cov-2",
  language  = "en"
}

@ARTICLE{Ali2021-ep,
  title     = "Novel therapeutic drug strategies to tackle immune-oncological
