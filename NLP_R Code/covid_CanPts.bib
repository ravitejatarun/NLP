  month     =  dec,
  year      =  2021,
  keywords  = "Alveolar Epithelial Cells; COVID-19; Endothelium;
               Renin-Angiotensin System; Respiratory Distress Syndrome",
  language  = "en"
}

@ARTICLE{Kaur2021-wt,
  title     = "Transforming challenges into opportunities: conducting health
               preference research during the {COVID-19} pandemic and beyond",
