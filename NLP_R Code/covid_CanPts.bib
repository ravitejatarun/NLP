  pages     = "1109--1117",
  month     =  dec,
  year      =  2021,
  keywords  = "COVID-19, coronavirus disease 2019; IRR, incidence rate ratio",
  copyright = "http://creativecommons.org/licenses/by-nc-nd/4.0/",
  language  = "en"
}

@ARTICLE{El_Khoury2021-nm,
  title     = "Patient-centered home cancer screening attitudes during
               {COVID-19} pandemic",
  author    = "El Khoury, Christelle and Haro, Elizabeth and Alves, Martha and
               O'Dwyer, Marie Claire and Meixner, Kate and Albiac, Laura Crespo
               and Capizzano, J Nicoll and Ramakrishnan, Manasi and Salada,
