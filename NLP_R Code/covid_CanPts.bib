  volume    =  10,
  number    =  24,
  pages     = "8763--8767",
  month     =  dec,
  year      =  2021,
  keywords  = "Covid-19; PCR testing; asymptomatic; cancer; screening",
  copyright = "http://creativecommons.org/licenses/by/4.0/",
  language  = "en"
}

