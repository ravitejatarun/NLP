  pages     = "6071",
  month     =  dec,
  year      =  2021,
  keywords  = "COVID-19; diagnostic delay; immunotherapy; lymph adenopathy;
               management; melanoma; pandemic; teledermatology; vaccination",
  copyright = "https://creativecommons.org/licenses/by/4.0/",
  language  = "en"
}

@ARTICLE{De_Vita2021-be,
