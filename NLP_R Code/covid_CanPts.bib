@ARTICLE{Kirchberg2021-xd,
  title     = "Influence of the first wave of the {COVID-19} pandemic on cancer
               care in a German Comprehensive Cancer Center",
  author    = "Kirchberg, Johanna and Rentsch, Anke and Klimova, Anna and Vovk,
