  copyright = "https://creativecommons.org/licenses/by/4.0/",
  language  = "en"
}

@ARTICLE{Javadinia2021-xo,
  title     = "Multicenter study of antibody seroprevalence against {COVID-19}
               in patients presenting to Iranian cancer centers after one year
               of the {COVID-19} pandemic",
  author    = "Javadinia, Seyed Alireza and Ariamanesh, Mona and Nabavifard,
