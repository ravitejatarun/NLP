@ARTICLE{Wong2021-uv,
  title     = "Systemic anti-cancer therapy and metastatic cancer are
               independent mortality risk factors during two {UK} waves of the
               {COVID-19} pandemic at university college London hospital",
  author    = "Wong, Yien Ning Sophia and Sng, Christopher C T and Ottaviani,
