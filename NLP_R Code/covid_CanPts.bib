  volume    =  11,
  pages     = "761393",
  month     =  nov,
  year      =  2021,
  keywords  = "elderly; elderly oncology; geriatric oncology; head and neck
               cancer; hypofractionation; radiotherapy",
  copyright = "https://creativecommons.org/licenses/by/4.0/",
  language  = "en"
}

