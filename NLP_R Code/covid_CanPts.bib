@ARTICLE{Boscolo-Rizzo2021-pp,
  title     = "High prevalence of long-term olfactory, gustatory, and
               chemesthesis dysfunction in {post-COVID-19} patients: a matched
               case-control study with one-year follow-up using a comprehensive
