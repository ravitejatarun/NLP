  month     =  oct,
  year      =  2021,
  keywords  = "COVID-19; infection control; ophthalmologists",
  language  = "en"
}

@ARTICLE{Elsamany2021-np,
  title     = "Suggested modifications to the management of patients with
               breast cancer during the {COVID-19} pandemic: Web-based survey
