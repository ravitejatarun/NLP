  publisher = "Springer Science and Business Media LLC",
  volume    =  39,
  number    =  11,
  pages     = "4085--4099",
  month     =  nov,
  year      =  2021,
  keywords  = "COVID-19; Deferred; PCa; Prostate cancer; RP; Radical
               prostatectomy",
  copyright = "https://creativecommons.org/licenses/by/4.0",
  language  = "en"
}

