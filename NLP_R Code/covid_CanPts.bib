@ARTICLE{De_Jong2021-jh,
  title     = "Quantification of {anti-SARS-CoV-2} antibodies in human serum
               with {LC-QTOF-MS}",
  author    = "de Jong, Karen A M and Rosing, Hilde and Vermunt, Marit and
