@ARTICLE{Verhagen2021-ka,
  title     = "Human {CD4+} {T} cells specific for dominant epitopes of
               {SARS-CoV-2} Spike and Nucleocapsid proteins with therapeutic
               potential",
  author    = "Verhagen, Johan and van der Meijden, Edith D and Lang, Vanessa
