@ARTICLE{Cusi2021-nt,
  title    = "Hyperimmune plasma in three immuno-deficient patients affected by
              non-severe, prolonged {COVID-19}: a single-center experience",
  author   = "Cusi, Maria Grazia and Conticini, Edoardo and Gandolfo, Claudia
