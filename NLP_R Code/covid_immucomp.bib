@ARTICLE{Wang2021-tq,
  title     = "Comparative assessment of favipiravir and remdesivir against
               human coronavirus {NL63} in molecular docking and cell culture
               models",
  author    = "Wang, Yining and Li, Pengfei and Rajpoot, Sajjan and Saqib, Uzma
