  year     =  2021,
  keywords = "COVID-19; Epidural; Hematoma; Spinal; Spontaneous",
  language = "en"
}

@ARTICLE{Panda2021-vf,
  title     = "Management of {COVID-19-related} severe acute respiratory
               distress syndrome in a patient with active human
               immunodeficiency virus infection",
  author    = "Panda, Rajesh and Singh, Pooja and Saigal, Saurabh and Karna,
               Sunaina T",
  abstract  = "Our understanding of the risk of developing severe coronavirus
               disease 2019 (COVID-19) infection in the population living with
               HIV is still evolving. In this report, we discuss the successful
               management of COVID-19-related acute respiratory distress
               syndrome (ARDS) in an active case of the human immunodeficiency
               virus (HIV) infection; the patient had an atypical presentation
               characterized by very rapid progression in the severity of
               respiratory symptoms needing invasive mechanical ventilation.
