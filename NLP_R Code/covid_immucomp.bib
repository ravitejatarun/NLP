  keywords  = "microbemias; oral health; periodontal disease; susceptible host;
               systemic disease; systemic inflammation",
  copyright = "http://creativecommons.org/licenses/by-nc-nd/4.0/",
  language  = "en"
}

@ARTICLE{Blasi2021-rb,
