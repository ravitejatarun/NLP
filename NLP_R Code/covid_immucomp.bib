@ARTICLE{Garcia-Guillen2021-vb,
  title     = "{COVID-19}: Overview of rheumatology fellows",
  author    = "Garcia-Guill{\'e}n, Andrea and Jeria, Sicylle and Lobo-Prat,
               David and Sainz, Lu{\'\i}s",
  abstract  = "SARS-COV-2 infection has spread worldwide since it originated in
