@ARTICLE{Ghazizadeh_Esslami2021-zk,
  title    = "{COVID-19} in transplant recipient children: An Iranian referral
              hospital-based study",
  author   = "Ghazizadeh Esslami, Golnaz and Mamishi, Setareh and Mahmoudi,
