  volume    =  17,
  number    =  1,
  pages     = "29",
  month     =  mar,
  year      =  2021,
  keywords  = "Allergy; Anaphylaxis; COVID-19; Immune deficiency;
               Immunocompromise; PEG; Vaccine",
  copyright = "http://creativecommons.org/licenses/by/4.0/",
