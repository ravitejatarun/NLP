  volume    =  186,
  number    =  5,
  pages     = "589--608",
  month     =  oct,
  year      =  2021,
  keywords  = "Aspergillus fumigatus; Genomics; Mycoses; Respiratory diseases",
  copyright = "https://creativecommons.org/licenses/by/4.0",
  language  = "en"
}

