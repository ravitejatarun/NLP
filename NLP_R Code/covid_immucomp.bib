  keywords  = "Hormones; Inflammation; Ovarian cancer; SARS-CoV-2; risk factor",
  copyright = "http://creativecommons.org/licenses/by/4.0/",
  language  = "en"
}

@ARTICLE{Stefan2021-pz,
